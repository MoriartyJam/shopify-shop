<div class="page-width">
  {% if section.settings.get_image_v_1 != blank %}
    <div id="get_image_v_1">
      <img src="{{section.settings.get_image_v_1 | img_url: 'icon'}}"  class="lazyload" alt="{{section.settings.get_image_v_1.alt}}">
      <br>
      <img src="{{section.settings.get_image_v_1 | img_url: '500'}}"  class="lazyload" alt='{{section.settings.get_image_v_1.alt}}'>
      <br>
      <img src="{{section.settings.get_image_v_1 | img_url: 'large'}}"  loading="lazy" alt='{{section.settings.get_image_v_1.alt}}'>
      <br>
      {{ section.settings.get_image_v_1 | img_url:'510x' | img_tag }}
      {% render 'icons', icon: 'tiktok' %}
      </div>
  {% endif %}
    <div class="get_image_v_3">

    <img src="{{'image-leopard-assets.jpg' | asset_url}}" alt="leopard">
    </div>
    <div class="shopify_logo">
          <img src="{{'shopify.svg' | asset_url}}" alt="leopard">


    
    </div>


    
    
  
</div>

{% schema %}
{
  "name": "Image get links",
  "tag": "section",
  "class": "section",
  "settings": [ 
    {
  "type": "image_picker",
  "id": "get_image_v_1",
  "label": "Get image v1"
}, 
  {
  "type": "image_picker",
  "id": "get_image_v_2",
  "label": "Image_picker without image"
}
  ],
  "blocks": [
  ],
  "presets": [
    {
      "name": "Image get links"
    }
  ]
}
{% endschema %}
